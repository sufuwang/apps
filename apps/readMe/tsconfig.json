{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@lib/*": ["./src/lib/*"],
      "@hooks/*": ["./src/lib/hooks/*"],
      "@db/*": ["./src/lib/database/*"],
      "@actions/*": ["./src/lib/actions/*"],
      "@ui/*": ["./src/components/ui/*"],
      "@comps/*": ["./src/components/*"],
      "@mdx/*": ["./src/mdx/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "1mdx-components.ts", "src/app/economics/_comps/.Earning.tsx"],
  "exclude": ["node_modules"]
}
